{
	"name": "vscode-zedritsch-theme",
	"displayName": "Zedritsch Theme",
	"description": "VS Code all-in-one Theme",
	"version": "1.0.0",
	"publisher": "Zedritsch",
	"author": {
		"name": "Zedritsch",
		"email": "contact.zedritsch@gmail.com",
		"url": "https://zedritsch.tk"},
	"engines": { "vscode": "^1.53.0" },
	"homepage": "https://zedritsch.tk/Zedritsch-Theme",
	"repository": {
		"type": "git",
		"url": "https://github.com/Zedritsch/Zedritsch-Theme.git"
	},
	"icon": "./assets/logo.png",
	"license": "MIT",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dark",
		"light",
		"icons",
		"theme",
		"themes",
		"vscode",
		"icon-theme",
		"customization"
	],
	"contributes": {
		"themes": [
			{
				"id": "vs-zedritsch-light-theme",
				"label": "Zedritsch Light Theme",
				"uiTheme": "vs",
				"path": "./themes/light.json"
			},
			{
				"id": "vs-zedritsch-dark-theme",
				"label": "Zedritsch Dark Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/dark.json"
			}
		],
		"iconThemes": [{
			"id": "vs-zedritsch-icon-theme",
			"label": "Zedritsch Icon Theme",
			"path": "./themes/icons.json"
		}]
	}
}